{
    "manifest_version": 3,
    "name": "Close Window",
    "version": "1.0",
    "description": "Close",
    "permissions": ["activeTab" ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "48": "icon.png"
    },
    "browser_action": {
      "default_popup": ""
    }
  }
  